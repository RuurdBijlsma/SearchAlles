{
  "name": "electron-template",
  "version": "0.0.1",
  "description": "Electron template by Ruurd Bijlsma",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "build"
  },
  "repository": "https://github.com/ruurdbijlsma/temporary",
  "homepage": "https://ruurdbijlsma.com",
  "keywords": [
    "Electron",
    "template"
  ],
  "author": {
    "name": "Ruurd Bijlsma",
    "email": "mail@ruurdbijlsma.com"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^19.55.3"
  },
  "dependencies": {
    "electron-is-dev": "^0.1.2"
  },
  "build": {
    "appId": "com.ruurdbijlsma.template",
    "productName": "Electron Template",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS ",
      "icon": "res/img/icon.ico"
    }
  }
}
